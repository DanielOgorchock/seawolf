utilities_xmdf::xmdfSetData $instance FileSet $fcount relative_path sram_bank.ngc
utilities_xmdf::xmdfSetData $instance FileSet $fcount type ngc
incr fcount

utilities_xmdf::xmdfSetData $instance FileSet $fcount associated_module sram_bank
incr fcount

}

# ::gen_comp_name_xmdf::xmdfApplyParams
